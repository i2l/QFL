name:                QFL
version:             0.1.0.0
synopsis:            Quantum Foobar Language
description:         Just another useless Turing tarpit
license:             PublicDomain
license-file:        LICENSE
category:            Language
build-type:          Simple
cabal-version:       >=1.10

executable QFL
  main-is:             Main.hs
