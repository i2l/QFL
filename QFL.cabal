  build-depends:       base >=4.7 && <4.8, transformers >=0.4 && <0.5, random >=1.1 && <1.2, unix >=2.7 && <2.8, containers >=0.5 && <0.6, parsec >=3.1 && <3.2
  -- hs-source-dirs:      
  default-language:    Haskell2010